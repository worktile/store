{
  "name": "@tethys/store",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:docs",
    "docgeni": "npx --no-install docgeni init",
    "start:docs": "docgeni serve --port 8887",
    "build": "ng build store --prod",
    "build:docs": "docgeni build --prod",
    "build:docs-gh-pages": "docgeni build --prod --base-href=/docgeni-template/",
    "test": "ng test",
    "report-coverage": "cat ./coverage/store/lcov.info | coveralls",
    "lint": "ng lint",
    "prepare": "husky install",
    "pretty": "pretty-quick",
    "release": "wpm release",
    "pub": "wpm publish",
    "pub-only": "npm publish ./dist/store",
    "preinstall": "node scripts/check-yarn.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@tethys/cdk": "^11.0.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-angular": "^12.0.1",
    "@docgeni/cli": "^1.0.7",
    "@docgeni/template": "^1.0.7",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@worktile/pkg-manager": "0.0.1",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.0.9",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
